            <string>/// Animate the title

var baseDestScal = 0;
var baseDestAngl = 0;

if(!m_intro) { baseDestScal = 2;}

if(instance_exists(obj_charSelectManager))
{
    m_titleY = lerp(m_titleY, room_height/8, 0.2);
    baseDestScal = 1;
    baseDestAngl = -360;
}

m_titleBaseScale = lerp(m_titleBaseScale, baseDestScal, 0.1);
m_titleBaseAngle = lerp(m_titleBaseAngle, baseDestAngl, 0.1);

m_titleAngle = m_titleBaseAngle + (sin(current_time / 1000) * 10);

var time = audio_sound_get_track_position(m_bgm);

var bpm = 100;
var currentPos = time * (bpm / 60);

var timer = frac(currentPos);
var timer = frac(currentPos);

if(floor(currentPos) % 2 == 0)
{
    m_titleXScale = ease_out_back(timer, 1.5, -0.5, 1) * m_titleBaseScale;
    m_titleYScale = m_titleBaseScale;
}
else
{
    m_titleXScale = m_titleBaseScale;
    m_titleYScale = ease_out_back(timer, 1.5, -0.5, 1) * m_titleBaseScale;   
}


</string>
